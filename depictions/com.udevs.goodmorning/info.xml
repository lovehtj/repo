<package>
	<id>com.udevs.goodmorning</id>
	<name>GoodMorning</name>
	<version>1.3.1</version>
	<compatibility>
		<firmware>
			<miniOS>10.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
		<package>mobilesubstrate</package>
	</dependencies>
	<shortDescription>Simple command runner every morning!</shortDescription>
	<descriptionlist>
		<description><![CDATA[
		This tweak will run the command if your device woke up after been idled for specified duration since last lock.
		]]>
		</description>
	</descriptionlist>
	<screenshots>
		<screenshot>
			<description></description>
			<image></image>
		</screenshot>
	</screenshots>
	<changelog>
		<change>Updated for iOS 13.</change>
	</changelog>
	<links>
		<link>
			<name>/r/jailbreak</name>
			<url>https://www.reddit.com/r/jailbreak</url>
			<iconclass>fa fa-reddit</iconclass>
		</link>
	</links>
</package>

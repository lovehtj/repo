<package>
	<id>com.udevs.dockx</id>
	<name>DockX</name>
	<version>1.0</version>
	<compatibility>
		<firmware>
			<miniOS>12.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
		<package>mobilesubstrate</package>
        <package>rocketbootstrap</package>
	</dependencies>
	<shortDescription>Pasteboard shortcuts on your dock!</shortDescription>
	<descriptionlist>
		<description><![CDATA[
            Supporting notched devices (X,XS,XR,11) running iOS 12-13.
            Available shortcuts:
                Select All
                Copy
                Paste (limited paste feature on iOS 12 for images)
                Cut
                Undo
                Redo
            and long press on
                Select All to select last word
                Cut to delete last word
                Undo to go to beginning of document
                Redo to go to beginning of document
            Go to setting page to reorder your shortcuts.
		]]>
		</description>
	</descriptionlist>
	<screenshots>
		<screenshot>
			<description></description>
			<image></image>
		</screenshot>
	</screenshots>
	<changelog>
		<change>Initial Release</change>
	</changelog>
	<links>
		<link>
			<name>/r/jailbreak</name>
			<url>https://www.reddit.com/r/jailbreak</url>
			<iconclass>fa fa-reddit</iconclass>
		</link>
	</links>
</package>
